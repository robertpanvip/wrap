{
    "env": {
        "development": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": {
                          "browsers": ["ie >= 9", "chrome >= 62"]
                        }
                    }
                ],
                [
                    "@babel/preset-react"
                ]
            ],
            "plugins": [
                [
                    "@babel/plugin-proposal-decorators",
                    {
                        "legacy": true
                    }
                ],
                [
                    "@babel/plugin-proposal-class-properties",
                    {
                        "loose": true
                    }
                ],
                [
                    "@babel/plugin-transform-runtime"
                ]
            ]
        }
    }
}

